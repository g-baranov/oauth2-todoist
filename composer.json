{
    "name": "artisangora/oauth2-todoist",
    "description": "Todoist OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "todoist"
    ],
    "require": {
        "php": "^8.0",
        "league/oauth2-client": "^2.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Artisangora\\OAuth2Todoist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisangora\\OAuth2Todoist\\Tests\\": "tests/"
        }
    }
}
